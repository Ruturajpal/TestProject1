type Post {
    id: ID!
    title: String!
    content: String!
}

type Query {
    getAllPosts: [Post]
    getPostById(id: ID!): Post
}

type Mutation {
    createPost(title: String!, content: String!): Post
    updatePost(id: ID!, title: String!, content: String!): Post
    deletePost(id: ID!): String
}
